public class type {
private int id ; 
private int cout ; 
private int longmax ;
private int longmin ; 
private int resmaxtension ;
private int resmaxcompression ; 


public type (int id , int cout, int longmax , int longmin , int resmaxtension , int resmaxcompression){
this.id=id;
this.cout=cout; 
this.longmin = longmin ; 
this.longmax = longmax ;
this.resmaxtension = resmaxtension ; 
this.resmaxcompression = resmaxcompression ;
}

public class Barre {
private int id ; 
private noeud n1 ;
private noeud n2 ; 
private type typbarre ; 

public barre (noeud n1, noeud n2 , type typebarre  ){
this.n1= n1;
this.n2= n2 ; 
this.id = id ;
this.typbarre = typbarre ; 
}
